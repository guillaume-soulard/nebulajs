          "min": 1,
          "max": 9999999999999999
        },
        "sequence": {
          "enable": true,
          "cycle": false
        }
      }
    },
    "firstName": {
      "_type": "string",
      "options": {
        "pattern": "[A-Z]{1}[a-z]{5,25}"
      }
    },
